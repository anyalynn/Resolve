@model Resolve.Models.Case

@foreach (var item in Model.SampleCaseType)
{
    <dl class="row">
        <dt class="col-sm-4">
            @Html.DisplayNameFor(modelItem => item.CaseDescription)
        </dt>
        <dd class="col-sm-8">
            @Html.DisplayFor(modelItem => item.CaseDescription)
        </dd>
        <dt class="col-sm-4">
            @Html.DisplayNameFor(modelItem => item.EmployeeName)
        </dt>
        <dd class="col-sm-8">
            @Html.DisplayFor(modelItem => item.EmployeeName)
        </dd>
        <dt class="col-sm-4">
            @Html.DisplayNameFor(modelItem => item.Salary)
        </dt>
        <dd class="col-sm-8">
            @Html.DisplayFor(modelItem => item.Salary)
        </dd>
    </dl>
}