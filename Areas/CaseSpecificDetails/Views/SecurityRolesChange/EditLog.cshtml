@model Resolve.Models.SecurityRolesChangeTracking

@{
    ViewData["Title"] = "Specific Details Edit Log";
}

<h3>Edit Log</h3>

<h4>SecurityRolesChange Edit Tracking</h4>
<hr />

@{
    var logs = ViewData["Logs"] as List<SecurityRolesChangeTracking>;

    if (logs != null)
    {
    <table class="table table-hover table-sm">
        <thead>
            <tr>
                <th class="th-sm">Value Type</th>
                <th class="th-sm">ReqType</th>
                <th class="th-sm">Worker Type</th>
                <th class="th-sm">Hire Type</th>
                <th class="th-sm">Department</th>
                <th class="th-sm">SupOrg</th>
                <th class="th-sm">Name</th>
                <th class="th-sm">Job Title</th>
                <th class="th-sm">Employee EID</th>
                <th class="th-sm">UWNetID</th>
                <th class="th-sm">Effective Date</th>
                <th class="th-sm">Supervised Access</th>
                <th class="th-sm">Include Subs</th>
                <th class="th-sm">Academic Chair</th>
                <th>Academic Dean</th>
                <th>HCM Init1</th>
                <th>HCM Init2</th>
                <th>I9</th>
                <th>Manager</th>
                <th>UWHires Manager</th>
                <th>VOStaff</th>
                <th>VOStaff CompCost</th>
                <th>VOStaff CompPersonal</th>
                <th>Time Absence Approver</th>
                <th>Time Absence Initiate</th>
            </tr>
        </thead>
       
        <tbody>
            @foreach (var item in logs)
            {
            <tr>
                <td><b>@item.Status</b></td>
                <td>@item.SecurityRequestType</td>
                <td>@item.AWorkerType</td>
                <td>@item.HireType</td>
                <td>@item.Department</td>                }
                <td>@item.SupOrg</td>
                <td>@item.Name</td>
                <td>@item.JobTitle</td>
                <td>@item.EmployeeEID</td>
                <td>@item.UWNetID</td>
                <td>@item.EffectiveDate</td>
                <td>@item.SupervisedAccess</td>
                <td>@item.IncludeSubordinates</td>
                <td>@item.AcademicChair</td>
                <td>@item.AcademicDean</td>
                <td>@item.HCMInit1</td>
                <td>@item.HCMInit2</td>
                <td>@item.I9</td>
                <td>@item.Manager</td>
                <td>@item.UWHiresManager</td>
                <td>@item.VOStaff</td>
                <td>@item.VOStaffCompCost</td>
                <td>@item.VOStaffCompPersonal</td>
                <td>@item.TimeAbsenceApprover</td>
                <td>@item.TimeAbsenceInitiate</td>

            </tr>
            }
        </tbody>
    </table>
    }
    else
    {
        <p>No logs found currently</p>
    }
}


